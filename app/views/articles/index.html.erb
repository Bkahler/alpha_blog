<h1>ALL ARTICLES</h1>
<p>
  <%= link_to "New Article", new_article_path %>
</p>
<table>
    <tr>
        <th>Title</th>
        <th>Description</th>
    </tr>
    <% @articles.each do |article| %>
        <tr>
            <td><%= article.title %> </td>
            <td><%= article.description %> </td>
            <td> <%= link_to "See article", article_path(article) %> </td>
            <td> <%= link_to "Edit article", edit_article_path(article) %> </td>
        </tr>
    <% end %>
</table>